{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "RateLimitConfig": [
    {
      "Type": "Anonymous",
      "TimeWindow": 1,
      "MaxRequests": 5
    },
    {
      "Type": "Referer",
      "TimeWindow": 1,
      "MaxRequests": 10
    },
    {
      "Type": "Authenticated",
      "TimeWindow": 1,
      "MaxRequests": 50
    }
  ]
}
